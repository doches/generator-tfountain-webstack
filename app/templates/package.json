    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "classnames": "^2.2.6",
    <% if (redux) { %>
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "react-router-dom": "^5.2.0",
    "react-router-redux": "^4",
    "react-router": "^5.2.0",
